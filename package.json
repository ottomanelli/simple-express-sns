{
  "name": "simple-express-sns",
  "version": "1.0.0",
  "description": "A basic express server with one route that is used to hit a aws sns service",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "keywords": [
    "aws",
    "sns",
    "text",
    "express"
  ],
  "author": "Frankie Ottomanelli",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1185.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
